{
  "name": "task-lite",
  "version": "1.0.0",
  "scripts": {
    "dev": "node ../../../tools/buildlite/src/index.js default dev",
    "build": "node ../../../tools/buildlite/src/index.js default build"
  },
  "buildlite": {
    "default": {
      "entry": "./src/setup/index.jsx",
      "outputPath": "dist",
      "outputName": "[name].[hash].js",
      "style": "style.[hash].css",
      "html": {
        "templatePath": "./public/index.html"
      },
      "copy": [
        {
          "from": "./public",
          "to": "."
        }
      ]
    },
    "dev": {
      "react": "dev",
      "devServer": 8000
    },
    "build": {
      "react": "build",
      "clean": [
        "dist"
      ]
    }
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "history": "^4.7.2",
    "material-ui": "^1.0.0-beta.37",
    "material-ui-icons": "^1.0.0-beta.36",
    "ramda": "^0.25.0",
    "react": "^16.1.1",
    "react-autocomplete": "^1.7.2",
    "react-dom": "^16.1.1",
    "react-flip-move": "^3.0.1",
    "react-hot-loader": "^4.1.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-observable": "^0.17.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.2",
    "superagent": "^3.8.1",
    "typeface-roboto": "^0.0.45"
  }
}
